{
  "name": "github-tag-release",
  "version": "0.0.5",
  "private": true,
  "description": "A actions that can automatically generate release",
  "main": "dist",
  "scripts": {
    "build": "rm -Rf dist && ncc build src/index.ts --license licenses.txt",
    "prebuild": "rm -Rf dist",
    "postbuild": "git add dist",
    "lint": "eslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janrywang/github-tag-release.git"
  },
  "keywords": [
    "actions",
    "node",
    "auto-release"
  ],
  "resolutions": {
    "yargs": "^16.x",
    "commander": "^6.x"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "@actions/exec": "^1.1.0",
    "semver": "^7.3.5",
    "gh-release": "^6.0.1",
    "fs-extra": "^10.0.0",
    "moment": "^2.29.1",
    "string-similarity": "^4.0.4",
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.25.1",
    "@types/node": "^16.10.5",
    "@types/semver": "^7.3.9",
    "@types/fs-extra": "^9.0.13",
    "@typescript-eslint/parser": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "eslint-plugin-markdown": "^2.0.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "ghooks": "^2.0.4",
    "lint-staged": "^8.2.1",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/prompt-cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.4.4"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run build && lint-staged",
      "commit-msg": "commitlint --edit"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --ext .ts,.tsx,.js",
      "pretty-quick --staged",
      "git add"
    ],
    "*.md": [
      "pretty-quick --staged",
      "git add"
    ]
  }
}
